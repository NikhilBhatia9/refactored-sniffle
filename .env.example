# Alpha Oracle Configuration

# API Keys (Optional - platform works with demo data if not provided)
# Get Alpha Vantage key at: https://www.alphavantage.co/support/#api-key
ALPHA_VANTAGE_API_KEY=

# Get FRED API key at: https://fred.stlouisfed.org/docs/api/api_key.html
FRED_API_KEY=

# Application Settings
APP_NAME=Alpha Oracle
APP_VERSION=1.0.0
DEBUG=false

# Data Refresh Settings
DATA_REFRESH_INTERVAL=3600  # seconds
CACHE_TTL=300  # seconds

# Database
DB_URL=sqlite+aiosqlite:///./alpha_oracle.db

# CORS Origins (comma-separated)
CORS_ORIGINS=http://localhost:5173,http://localhost:3000,http://127.0.0.1:5173
